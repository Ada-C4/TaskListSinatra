<div>
  <h1>Task List</h1>
    <ul>
      <% @tasks.each do |task| %>
        <li>
          task: <%= task[0] %> description: <%= task[1] %> completed: <%= task[2] %>
        </li>
      <% end %>
    </ul>

    <a href="./new">Add a task here</a>
    <a href="./modify">Edit a task here</a>

</div>
